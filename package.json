{
  "name": "tim3d",
  "version": "0.0.0-development",
  "author": "Matej Hako≈°",
  "license": "Apache-2.0",
  "description": "Three.js interactive manual.",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --config webpack.config.prod.js",
    "dev": "webpack serve",
    "commit": "git add . && git-cz",
    "lint:check": "eslint -c .eslintrc.json ./src",
    "lint:fix": "eslint -c .eslintrc.json ./src --fix",
    "prettier:write": "prettier --config .prettierrc --write .",
    "prettier:check": "prettier --config .prettierrc --check .",
    "docs:gen": "typedoc --entryPoints ./src/index.ts",
    "prepare": "npm run build:prod",
    "semantic-release": "semantic-release --branches main"
  },
  "main": "./dist/tim3d.js",
  "types": "./src/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mattjoke/TIM3D.git"
  },
  "devDependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@types/clone-deep": "^4.0.1",
    "@types/hbs": "^4.0.1",
    "@types/three": "^0.125.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^3.1.0",
    "semantic-release": "^19.0.2",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^8.0.17",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.6.4",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "three": "latest",
    "typedoc": "^0.22.12",
    "zod": "^3.11.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
